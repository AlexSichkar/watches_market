<div id="<%= dom_id watch %>">
  <p>
    <strong>Name:</strong>
    <%= watch.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= watch.description %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= watch.category_id %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= watch.price %>
  </p>

  <p>
    <strong>View url:</strong>
    <%= watch.view_url %>
  </p>

</div>
